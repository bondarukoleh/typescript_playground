{
  "compilerOptions": {
    "target": "es6",
    "outDir": "./dist",
    "rootDir": "./src",
    "module": "commonjs",
    "lib": ["es5", "dom", "es2015.collection"],
    "declaration": true,
//    "strictNullChecks": true,
    "allowJs": true,
    "jsx": "react",
    "jsxFactory": "createElement",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": ["webpack.config.js", "./dist"]
}